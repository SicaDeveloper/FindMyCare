        "type": "string",
        "required": true
    },
    "phone": {
        "type": "string",
        "required": true
    },
    "gender": {
        "type": "string",
        "required": true
    },
    "panNumber": {
        "type": "string",
        "required": true
    },
    "dateOfBirth": {
        "type": "string", 
        "required": true
    },
    "citizenshipNumber": {
        "type": "string",
        "required": true
    },
    "citizenshipIssuedDate": {
        "type": "string", 
        "required": true
    },
